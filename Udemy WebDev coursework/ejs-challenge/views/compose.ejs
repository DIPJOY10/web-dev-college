<%- include('partials/header') -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="">Title</label>
    <input class="form-control" autocomplete="off" type="text" name="postTitle" value="">
    <label for="">Post</label>
    <textarea class="form-control" name="postBody" rows="8" cols="30"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include('partials/footer') -%>
